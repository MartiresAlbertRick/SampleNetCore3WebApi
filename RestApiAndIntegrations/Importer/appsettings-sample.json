                    "Custom02": "CUSTOM_02",
                    "Custom03": "CUSTOM_03",
                    "Custom04": "CUSTOM_04"
                }
            }
        }
    },
    "Logging": {
        "NLog": {
            "IncludeScopes": false,
            "ParseMessageTemplates": true,
            "CaptureMessageProperties": true
        }
    },
    "NLog": {
        "autoreload": true,
        "internalLogLevel": "Info",
        "internalLogFile": "App_Data/Logs/console-example-internal.log",
        "throwConfigExceptions": true,
        "targets": {
            "console": {
                "type": "ColoredConsole",
                "layout": "${date}|${level:uppercase=true}|${message} ${exception:format=tostring}|${logger}|${all-event-properties}"
            },
            "file": {
                "type": "AsyncWrapper",
                "target": {
                    "wrappedFile": {
                        "type": "File",
                        "fileName": "App_Data/Logs/console-example.log",
                        "layout": {
                            "type": "JsonLayout",
                            "Attributes": [
                                {
                                    "name": "timestamp",
                                    "layout": "${date:format=o}"
                                },
                                {
                                    "name": "level",
                                    "layout": "${level}"
                                },
                                {
                                    "name": "logger",
                                    "layout": "${logger}"
                                },
                                {
                                    "name": "message",
                                    "layout": "${message:raw=true}"
                                },
                                {
                                    "name": "properties",
                                    "encode": false,
                                    "layout": {
                                        "type": "JsonLayout",
                                        "includeallproperties": "true"
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "rules": [
            {
                "logger": "*",
                "minLevel": "Trace",
                "writeTo": "File,Console"
            }
        ]
    }
}