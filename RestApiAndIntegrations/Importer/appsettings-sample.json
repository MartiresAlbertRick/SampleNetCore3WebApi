                    "Custom02": "CUSTOM_02",
                    "Custom03": "CUSTOM_03",
                    "Custom04": "CUSTOM_04"
                }
            },
            "Payment": {
                "Route": "api/payment-updates/bulk",
                "DefaultImportFilePath": "D:\\",
                "DefaultImportFileName": "(?i)Payment_([0-9]{8}-[0-9]{4}).*\\.csv",
                "FileNameIsRegex": true,
                "MatchCurrentDateAndFileNameDate": false,
                "MatchCurrentDateAndFileCreationDate": false,
