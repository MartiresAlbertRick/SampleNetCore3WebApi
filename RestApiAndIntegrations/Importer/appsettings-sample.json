                    "VendorBankAccountList": "VENDOR_BANK_ACCOUNT_LIST",
                    "PoRequiredYN": "PO_REQUIRED_YN",
                    "Custom01": "CUSTOM_01",
                    "Custom02": "CUSTOM_02",
                    "Custom03": "CUSTOM_03",
                    "Custom04": "CUSTOM_04"
                }
